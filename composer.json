{
    "name": "webgefaehrten/laravel-locking",
    "description": "Universal locking system with optimistic & pessimistic traits for Laravel",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "pusher/pusher-php-server": "^7.2",
        "laravel/horizon": "^5.0",
        "livewire/flux": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Webgefaehrten\\Locking\\": "src/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan locking:install || true"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webgefaehrten\\Locking\\Providers\\LockingServiceProvider"
            ]
        }
    }
}
