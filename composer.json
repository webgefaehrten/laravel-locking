{
    "name": "webgefaehrten/laravel-locking",
    "description": "Universal locking system with optimistic & pessimistic traits for Laravel",
    "type": "library",
    "require": {
        "php": ">=8.2",
        "laravel/framework": ">=11.0",
        "pusher/pusher-php-server": ">=7.2",
        "laravel/horizon": ">=5.0",
        "livewire/flux": ">=1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.11",
        "nunomaduro/larastan": "^2.9",
        "stancl/tenancy": "^3.8",
        "stancl/jobpipeline": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Webgefaehrten\\Locking\\": "src/"
        }
    },
    "scripts": {},
    "extra": {
        "laravel": {
            "providers": [
                "Webgefaehrten\\Locking\\Providers\\LockingServiceProvider"
            ]
        }
    },
    "suggest": {
        "stancl/tenancy": "Optional: Tenant-aware Broadcasting & Cleanup (v4)"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
          "name": "Webgefaehrten - Joshua Levin Scharrer",
          "email": "hallo@webgefaehrten.de"
        }
    ],
    "keywords": [
        "laravel",
        "locking",
        "pessimistic-locking",
        "optimistic-locking",
        "tenancy",
        "webgefaehrten"
    ],
    "support": {
        "issues": "https://github.com/webgefaehrten/laravel-locking/issues",
        "source": "https://github.com/webgefaehrten/laravel-locking"
    }
}
